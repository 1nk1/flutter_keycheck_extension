{
  "name": "flutter-testing-keys-inspector",
  "displayName": "Flutter Testing Keys Inspector",
  "description": "Professional testing keys management and validation for Flutter projects with intelligent code analysis",
  "version": "1.4.0",
  "publisher": "flutter-tools",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Testing",
    "Linters",
    "Other"
  ],
  "keywords": [
    "flutter",
    "testing",
    "keys",
    "automation",
    "dart",
    "validation",
    "code-analysis",
    "widget-testing"
  ],
  "galleryBanner": {
    "color": "#0175C2",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/flutter-testing-keys-inspector.git"
  },
  "homepage": "https://github.com/your-username/flutter-testing-keys-inspector#readme",
  "bugs": {
    "url": "https://github.com/your-username/flutter-testing-keys-inspector/issues"
  },
  "license": "MIT",
  "activationEvents": [
    "workspaceContains:**/pubspec.yaml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "flutterTestingKeys",
          "name": "Testing Keys",
          "icon": "$(add)"
        }
      ]
    },
    "commands": [
      {
        "command": "flutterTestingKeys.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "flutterTestingKeys.validate",
        "title": "Validate Keys",
        "icon": "$(check)"
      },
      {
        "command": "flutterTestingKeys.generateReport",
        "title": "Generate Report",
        "icon": "$(report)"
      },
      {
        "command": "flutterTestingKeys.addKey",
        "title": "Add New Key",
        "icon": "$(add)"
      },
      {
        "command": "flutterTestingKeys.goToDefinition",
        "title": "Go to Definition",
        "icon": "$(go-to-file)"
      },
      {
        "command": "flutterTestingKeys.openWidgetPreview",
        "title": "Flutter Testing Keys: Open Widget Preview",
        "icon": "$(preview)"
      },
      {
        "command": "flutterTestingKeys.qaAnalyzeMultiProject",
        "title": "Flutter Testing Keys QA: Multi-Project Analysis",
        "icon": "$(checklist)"
      },
      {
        "command": "flutterTestingKeys.qaAnalyzeDependencies",
        "title": "Flutter Testing Keys QA: Analyze Dependencies",
        "icon": "$(package)"
      },
      {
        "command": "flutterTestingKeys.qaAnalyzeRepositories",
        "title": "Flutter Testing Keys QA: Analyze External Repositories",
        "icon": "$(repo)"
      },
      {
        "command": "flutterTestingKeys.qaComprehensiveAnalysis",
        "title": "Flutter Testing Keys QA: Comprehensive Analysis",
        "icon": "$(pulse)"
      },
      {
        "command": "flutterTestingKeys.qaBrokenKeyDetection",
        "title": "Flutter Testing Keys QA: Broken Key Detection",
        "icon": "$(bug)"
      },
      {
        "command": "flutterTestingKeys.demonstrateNavigation",
        "title": "Flutter Testing Keys: Demonstrate Smart Navigation",
        "icon": "$(target)"
      },
      {
        "command": "flutterTestingKeys.testWidgetHighlighting",
        "title": "Flutter Testing Keys: Test Widget Highlighting",
        "icon": "$(paintcan)"
      },
      {
        "command": "flutterTestingKeys.analyzeCurrentFileContext",
        "title": "Flutter Testing Keys: Analyze Current File Context",
        "icon": "$(search-view)"
      },
      {
        "command": "flutterTestingKeys.showNavigationStats",
        "title": "Flutter Testing Keys: Show Navigation Statistics",
        "icon": "$(graph)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "flutterTestingKeys.refresh",
          "when": "view == flutterTestingKeys",
          "group": "navigation"
        },
        {
          "command": "flutterTestingKeys.validate",
          "when": "view == flutterTestingKeys",
          "group": "navigation"
        },
        {
          "command": "flutterTestingKeys.addKey",
          "when": "view == flutterTestingKeys",
          "group": "navigation"
        },
        {
          "command": "flutterTestingKeys.openWidgetPreview",
          "when": "view == flutterTestingKeys",
          "group": "navigation"
        },
        {
          "command": "flutterTestingKeys.qaComprehensiveAnalysis",
          "when": "view == flutterTestingKeys",
          "group": "qa"
        }
      ],
      "view/item/context": [
        {
          "command": "flutterTestingKeys.goToDefinition",
          "when": "view == flutterTestingKeys && viewItem == key",
          "group": "inline"
        }
      ]
    },
    "configuration": {
      "title": "Flutter Testing Keys Inspector",
      "properties": {
        "flutterTestingKeys.autoValidate": {
          "type": "boolean",
          "default": true,
          "description": "Automatically validate keys on file save"
        },
        "flutterTestingKeys.keyConstantsPath": {
          "type": "string",
          "default": "lib/constants/key_constants.dart",
          "description": "Path to KeyConstants file"
        },
        "flutterTestingKeys.showUnusedKeys": {
          "type": "boolean",
          "default": true,
          "description": "Show unused keys in the tree view"
        },
        "flutterTestingKeys.enableDiagnostics": {
          "type": "boolean",
          "default": true,
          "description": "Enable diagnostic messages for key issues"
        },
        "flutterTestingKeys.repositories.external": {
          "type": "array",
          "default": [],
          "description": "External repositories to analyze for testing keys",
          "items": {
            "type": "object",
            "properties": {
              "url": {
                "type": "string",
                "description": "Repository URL (GitHub or GitLab)"
              },
              "branch": {
                "type": "string",
                "default": "main",
                "description": "Branch to analyze"
              },
              "name": {
                "type": "string",
                "description": "Display name for the repository"
              },
              "enabled": {
                "type": "boolean",
                "default": true,
                "description": "Whether to include this repository in analysis"
              }
            },
            "required": [
              "url"
            ]
          }
        },
        "flutterTestingKeys.repositories.githubToken": {
          "type": "string",
          "default": "",
          "description": "GitHub personal access token for private repositories"
        },
        "flutterTestingKeys.repositories.gitlabToken": {
          "type": "string",
          "default": "",
          "description": "GitLab access token for private repositories"
        },
        "flutterTestingKeys.qa.enableMultiProjectAnalysis": {
          "type": "boolean",
          "default": true,
          "description": "Enable multi-project analysis for QA Engineers"
        },
        "flutterTestingKeys.qa.enableDependencyAnalysis": {
          "type": "boolean",
          "default": true,
          "description": "Enable external dependency analysis"
        },
        "flutterTestingKeys.qa.enableRepositoryAnalysis": {
          "type": "boolean",
          "default": false,
          "description": "Enable external repository analysis (requires configuration)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && webpack --config webpack.webview.config.js",
    "watch": "tsc -watch -p ./",
    "build-webview": "webpack --config webpack.webview.config.js",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package",
    "publish": "vsce publish",
    "webpack": "webpack --mode production --devtool hidden-source-map",
    "compile-web": "webpack",
    "watch-web": "webpack --watch"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "16.x",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "@vscode/vsce": "^3.5.0",
    "eslint": "^8.28.0",
    "glob": "^11.0.3",
    "mocha": "^11.7.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  }
}
